<!-- The primary key and foreign key infos are given seperately -->

<fields_info>
    <field>
        <field_name>field name</field_name>
        <!-- <path></path>   This is optional. Either we expect the user to give this to us. Or we can extract the xpath using the field_name -->
        <dataType>string</dataType>
        <morphing_methods>
            <morphing_method>
                <method_name>HASH</method_name>
                <method_parameters>
                    <parameter name="algorithm">SHA256</parameter>
                </method_parameters>
            </morphing_method>
        </morphing_methods>
    </field>
</fields_info>


<relations>
    <primaryKeys>
        <field_name></field_name>
        <field_name></field_name>
        <field_name></field_name>
        <field_name></field_name>
    </primaryKeys>
    <parentKeys>
        <parentKey>
            <field_name></field_name>
            <referedBy>
                <field_name></field_name>
                <field_name></field_name>
                <field_name></field_name>
                <field_name></field_name>
            </referedBy>
        </parentKey>
    <parentKeys>
</relations>